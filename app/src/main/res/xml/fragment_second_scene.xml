<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <OnClick
            motion:touchAnchorId="@+id/fab"
            motion:touchAnchorSide="top"
            motion:dragDirection="dragUp" />
        <KeyFrameSet>
            <KeyPosition
                motion:motionTarget="@+id/button_fifth"
                motion:framePosition="50"
                motion:percentX="0.87"
                motion:percentY="0.049" />
            <KeyPosition
                motion:motionTarget="@+id/button_sixth"
               motion:framePosition="50"
               motion:percentX="0.87"
               motion:percentY="0.049" />
           <KeyPosition
               motion:motionTarget="@+id/button_seventh"
               motion:framePosition="50"
               motion:percentX="0.87"
               motion:percentY="0.049" />
           <KeyPosition
               motion:motionTarget="@+id/button_eight"
               motion:framePosition="50"
               motion:percentX="0.87"
               motion:percentY="0.049" />
           <KeyPosition
               motion:motionTarget="@+id/button_nine"
               motion:framePosition="90"
               motion:percentX="0.000"
               motion:percentY="1.000" />
           <KeyPosition
               motion:motionTarget="@+id/button_ten"
               motion:framePosition="90"
               motion:percentX="0.000"
               motion:percentY="1.000" />
           <KeyPosition
               motion:motionTarget="@+id/button_eleven"
               motion:framePosition="90"
               motion:percentX="0.000"
               motion:percentY="1.000" />
           <KeyPosition
               motion:motionTarget="@+id/button_twelve"
               motion:framePosition="90"
               motion:percentX="0.000"
               motion:percentY="1.000" />

       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/button_first" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@id/button_first"
            android:id="@+id/button_second" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@id/button_second"
            android:id="@+id/button_third" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@id/button_third"
            android:id="@+id/button_forth" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@id/button_forth"
            android:id="@+id/button_fifth" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@id/button_fifth"
            android:id="@+id/button_sixth" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@id/button_sixth"
            android:id="@+id/button_seventh" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@id/button_seventh"
            android:id="@+id/button_eight" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="gone"
            android:id="@+id/button_nine" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@id/button_nine"
            android:visibility="gone"
            android:id="@+id/button_ten" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@id/button_ten"
            android:visibility="gone"
            android:id="@+id/button_eleven" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@id/button_eleven"
            android:visibility="gone"
            android:id="@+id/button_twelve" />
    </ConstraintSet>
</MotionScene>